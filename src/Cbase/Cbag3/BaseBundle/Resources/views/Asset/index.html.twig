{% extends '::base.html.twig' %}

{% block content %}
<div class="span9">
    <p>
    <ul>
        {% for asset in assets %}
        <li><img src="{{ asset(asset.webPath) }}" alt="{{ asset.description }}" /> <a href="{{ path('cbase_cbag3_base_asset_delete', {'id': asset.id} )}}">delete</a> </li>
        {% endfor %}
    </ul>
    </p>
</div>
<div class="span3">
    {% block content_sidebar %}
    <h2>Actions</h2>
    <ul>
        <li><a href="{{ path('cbase_cbag3_base_asset_create')}}">create new asset</a></li>
    </ul>
    {% endblock content_sidebar %}
</div>
{% endblock content %}